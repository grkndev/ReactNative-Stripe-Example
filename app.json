{
  "expo": {
    "name": "stripe-example",
    "slug": "stripe-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "stripe-example",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gweepcreative.stripeexample"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://stripe-test-react-native.expo.app"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "its.required.for.apple.pay",
          "publishableKey": "pk_test_51MTt1YGWih1Eis2OSEyZgWTpqDnimIqB0oGJu927bfaK3lmak8CesAKjMn9AUnHU77MFzGspY2bZMfVasQUTY7vb00yNw8c8cP"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router":{
        "origin": "https://stripe-test-react-native.expo.app"
      },
      "eas": {
        "projectId": "0aab2e04-a7b8-4c03-a190-7f1952c2b77f"
      }
    }
  }
}
